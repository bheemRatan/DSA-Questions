let arr = [32330,454,3643,6,3656,46,4,64,56,46,4,64,6,46,56];
let max = Math.max(arr[0],arr[1]);
let sMax = Math.min(arr[0],arr[1]);

for(let i =2;i<arr.length;i++){
    if(arr[i]>max){
        sMax = max;
        max = arr[i];
    }
    else if(arr[i]>sMax){
        sMax = arr[i];
    }
}
console.log(sMax,max);
